{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "FFmpeg": {
    "DefaultPath": "",
    "AllowAutoDetect": true
  },
  "Processing": {
    "DefaultParallelTasks": 1,
    "DefaultCompressionPercentage": 70,
    "DefaultVideoCodec": "libx264",
    "DefaultAudioCodec": "aac",
    "DefaultOutputPattern": "{name}_compressed{ext}"
  },
  "UI": {
    "Theme": "Default",
    "Language": "zh-CN"
  }
}